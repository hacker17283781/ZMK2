/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &kp A  &kp B  &kp C  &kp D
                &kp E  &kp F  &kp G  &kp H
                &kp I  &kp J  &kp K  &kp L
                &kp M  &kp N  &kp O  &kp NO

                /* Encoder (links) */
                &kp J       /* encoder left turn  */
                &kp K       /* encoder right turn */
            >;
        };
    };
};
